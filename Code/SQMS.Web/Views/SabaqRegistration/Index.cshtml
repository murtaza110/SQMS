@model IEnumerable<SQMS.Web.Models.SabaqRegistration>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            SabaqGroup
        </th>
        <th>
            MemberId
        </th>
        <th>
            SabaqStatu
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SabaqGroup.GroupName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemberId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SabaqStatu.SabaqStatusName)
