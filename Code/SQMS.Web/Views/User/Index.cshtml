        </th>
        <th>
            FirstName
        </th>
        <th>
            LastName
        </th>
        <th>
            DateOfBirth
        </th>
        <th>
            Address1
        </th>
        <th>
            Address2
        </th>
        <th>
            Phone1
        </th>
        <th>
            Phone2
        </th>
        <th>
            Email
        </th>
        <th>
            Region
        </th>
        <th>
            SecurityQuestion
        </th>
        <th>
            SecurityAnswer
        </th>
        <th>
            Password
        </th>
        <th>
            IsActive
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
